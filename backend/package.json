{
  "name": "muse-backend",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "migrate:up": "migrate -path ./migrations -database \"${DATABASE_URL}\" up",
    "migrate:down": "migrate -path ./migrations -database \"${DATABASE_URL}\" down",
    "migrate:down:1": "migrate -path ./migrations -database \"${DATABASE_URL}\" down 1",
    "migrate:create": "migrate create -ext sql -dir ./migrations -seq",
    "migrate:force": "migrate -path ./migrations -database \"${DATABASE_URL}\" force",
    "migrate:version": "migrate -path ./migrations -database \"${DATABASE_URL}\" version"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.25",
    "@ai-sdk/google": "^2.0.18",
    "@ai-sdk/mistral": "^2.0.18",
    "@ai-sdk/openai": "^2.0.46",
    "@ai-sdk/provider": "^2.0.0",
    "@ai-sdk/ui-utils": "^1.2.11",
    "@aws-sdk/client-s3": "^3.913.0",
    "@aws-sdk/s3-request-presigner": "^3.913.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/sensible": "^6.0.3",
    "@fastify/static": "^8.2.0",
    "@fastify/swagger": "^9.5.2",
    "@fastify/type-provider-typebox": "^6.0.0",
    "@fastify/websocket": "^11.2.0",
    "@muse/shared-schemas": "workspace:*",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@tldraw/sync-core": "^3.15.5",
    "@tldraw/tlschema": "^3.15.5",
    "@types/image-size": "^0.8.0",
    "ai": "^5.0.63",
    "better-auth": "^1.3.27",
    "dompurify": "^3.2.7",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.6",
    "fastify": "^5.6.1",
    "fastify-plugin": "^5.1.0",
    "image-size": "^2.0.2",
    "ioredis": "^5.8.2",
    "jsdom": "^27.0.0",
    "pg": "^8.16.3",
    "pino": "^10.0.0",
    "pino-pretty": "^13.1.1",
    "replicate": "^1.3.0",
    "resumable-stream": "^2.2.8",
    "undici": "^7.16.0",
    "unfurl.js": "^6.4.0",
    "uuid": "^13.0.0",
    "ws": "^8.18.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.19.19",
    "@types/pg": "^8.15.5",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "dotenv-cli": "^10.0.0",
    "drizzle-kit": "^0.20.18",
    "eslint": "^9.37.0",
    "node-pg-migrate": "^8.0.3",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
